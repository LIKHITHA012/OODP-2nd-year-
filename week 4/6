#include <iostream>
using namespace std;
    class number
{
	public:
		number(int i,int sum)
		{
	cout<<"numbers that are divisible by 9 between 100 and 200\n";
				for(i=101; i<200; i++)
				if(i%9==0)
				{
					cout<<"numbers divisble by 9 is= "<<i<<endl;
					sum=sum+i;
				}
				cout<<"Sum of all numbers divisible by 9 is= "<<sum<<endl;
		}
		~number()
		{
			cout<<"\n destructor "<<endl;
		}
};
int main()
{
	number f(101,0);
}

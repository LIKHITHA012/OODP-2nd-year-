#include <iostream> 
using namespace std;
int main(){
cout << "MENU\n";
cout <<"1. Addition " << endl;
cout <<"2. Subtraction" << endl; 
cout<<"3. Multiplation"<<endl;
cout<<"4. Division" << endl;
cout <<"5. Modulus "<< endl;
cout <<"Enter your Choice!!"<<endl;
int menu;
cin >> menu; 
int a,b;
cin>>a>> b;
switch (menu){
case 1:
cout << a+b;
break;
case 2:
cout << a-b;
break;
case 3: 
cout << a*b;
break;
case 4:
cout << a/b;
break;
case 5:
cout << a%b;
break;
default:
cout<<"Invaild Choice";
break;}
return 0;}
